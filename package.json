  "description": "An ECMAScript 6+ npm package demo",
  "main": "lib/md5.js",
  "keywords": [
    "ECMAScript"
  ],
  "author": "Anson Chen <ansoncat@gmail.com> (http://blog.ansoncat.com)",
  "license": "MIT",
  "homepage": "https://github.com/ansoncat/es6-npm-demo#readme",
  "repository": {
    "type": "git",
